              function FastDistance(unit1 ,unit2) {
                  var x1 = wread_epd(unit1 + 0x028 / 4,  0x028 % 4);
                  var y1 = wread_epd(unit1 + 0x02A / 4,  0x02A % 4);
                  var x2 = wread_epd(unit2 + 0x028 / 4,  0x028 % 4);
                  var y2 = wread_epd(unit2 + 0x02A / 4,  0x02A % 4);
                  var x;
                  var y;
                  var t;
                  if(
                      x1 < x2
                  ){
                      t = x1;
                      x1 = x2;
                      x2 = t;
                  }
                  if(
                      y1 < y2
                  ){
                      t = y1;
                      y1 = y2;
                      y2 = t;
                  }
                  x = x1-x2;
                  y = y1-y2;
                  if(
                      x < y
                  ){
                      t = x;
                      x = y;
                      y = t;
                  }
                  var d;
                  if(
                      y+y >= x
                  ){
                      d = y+y-x;
                  }
                  else{
                      d = 0;
                  }
                  d += y;
                  return x + d / 5;
              }
