                  */
                  var Owner;
                  var HeroType;
                  var Invincibility = 0;
                  Invincibility = dwread_epd(unitEPD + 0x0DC / 4);
                  if(
                      Invincibility & 0x4000000 >= 1
                  ){
                      return;
                  }
                  Owner = bread_epd(unitEPD + 0x04C / 4,  0x04C % 4);
                  HeroType = wread_epd(unitEPD + 0x064 / 4,  0x064 % 4);
                  var AcidSpore = 0;
